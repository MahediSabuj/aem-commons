<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured"
        cq:policy="aem-commons/components/page/policy"
        sling:resourceType="wcm/core/components/policies/mappings">
        <root jcr:primaryType="nt:unstructured"
            cq:policy="aem-commons/components/content/container/policy"
            sling:resourceType="wcm/core/components/policies/mapping">
            <experiencefragment-header jcr:primaryType="nt:unstructured"
                cq:policy="aem-commons/components/content/experience-fragment/policy_header"
                sling:resourceType="wcm/core/components/policies/mapping"/>
            <container jcr:primaryType="nt:unstructured"
                cq:policy="aem-commons/components/content/container/policy_main"
                sling:resourceType="wcm/core/components/policies/mapping">
                <container jcr:primaryType="nt:unstructured"
                    cq:policy="aem-commons/components/content/container/policy"
                    sling:resourceType="wcm/core/components/policies/mapping"/>
            </container>
            <experiencefragment-footer jcr:primaryType="nt:unstructured"
                cq:policy="aem-commons/components/content/experience-fragment/policy_footer"
                sling:resourceType="wcm/core/components/policies/mapping"/>
        </root>
    </jcr:content>
</jcr:root>
